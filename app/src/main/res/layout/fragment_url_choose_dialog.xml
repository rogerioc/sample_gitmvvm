<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable
            name="viewModel"
            type="com.rogerio.xingtest.feature.listRepos.presentation.dialog.UrlChooseDialogViewModel" />

        </data>

<FrameLayout
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context="com.rogerio.xingtest.feature.listRepos.presentation.dialog.UrlChooseDialog">
<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
    android:orientation="vertical">
    <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@string/repository"
            android:onClick="@{() -> viewModel.onRepoClicked()}"/>
    <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@string/owner"
            android:onClick="@{() -> viewModel.onOwnerClicked()}"/>
</LinearLayout>
</FrameLayout>
</layout>